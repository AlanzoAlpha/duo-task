pipeline {
    agent any
    stages{
        stage('Create docker images'){
            steps{
                // Make the images
                sh 'docker-compose up -d'
                // Check images
                sh 'doker images'
                
                }
            }
        stage('run python scripts') {
            steps {
                //Check container
                sh 'doker ps -a'
                // Map registry port
                //docker run -d -p 5000:5000 --name registry-duo registry
            }
        }
     }
}

//docker push registry-vm:5000/my/nginx
